@model List<Comic>
 
<h1 style="color:blue">
    Comics SQL Y ORACLE
</h1>
 
<a asp-controller="Comics"
    asp-action="Create">
    Create New
</a>
 
<table class="table table-bordered">
    <thead>
        <tr>
            <th>IdComic</th>
            <th>Nombre</th>
            <th>Imagen</th>
            <th>Descripcion</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (Comic comi in Model)
        {
            <tr>
                <td>@comi.IdComic</td>
                <td>@comi.Nombre</td>
                <td>@comi.Imagen</td>
                <td>@comi.Descripcion</td>
            </tr>
        }
    </tbody>
</table>